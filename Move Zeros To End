import java.io.*;
import java.util.*;
public static void swap(int arr[],int i,int j) {
		int temp=arr[i];
		arr[i]=arr[j];
		arr[j]=temp;
	}
public class Move Zeros To End{
public static void main(String args[]){
int arr[]= {1,2,3,0,8,0,4,0,12};
		int j=0;
		for(int i=0;i<arr.length;i++) {
			if(arr[i]!=0) {
				swap(arr,i,j);
				j++;
			}
		}
		for(int i=0;i<arr.length;i++) {
			System.out.print(arr[i]+"Â ");
	}
}
}
